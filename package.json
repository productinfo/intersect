{
  "name": "@tldraw/intersect",
  "version": "0.1.4",
  "description": "2D intersection utilities for TLDraw and maybe you, too.",
  "author": "@steveruizok",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tldraw/intersect.git"
  },
  "license": "MIT",
  "private": false,
  "keywords": [
    "2d",
    "vector",
    "intersection",
    "typescript",
    "javascript"
  ],
  "files": [
    "dist/**/*"
  ],
  "source": "src/index.ts",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "parcel watch",
    "build": "parcel build && tsc --project tsconfig.build.json",
    "check": "tsc --noEmit",
    "lint": "eslint",
    "prepublish": "yarn build"
  },
  "targets": {
    "main": {
      "includeNodeModules": {
        "@tldraw/vec": false
      }
    },
    "module": {
      "includeNodeModules": {
        "@tldraw/vec": false
      }
    }
  },
  "dependencies": {
    "@tldraw/vec": "^0.1.3"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.0.0",
    "@parcel/transformer-typescript-types": "^2.0.0",
    "parcel": "latest",
    "tslib": "^2.3.1",
    "typescript": ">=3.0.0"
  },
  "peerDependencies": {}
}